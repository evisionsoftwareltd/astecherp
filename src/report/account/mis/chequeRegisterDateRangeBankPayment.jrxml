	<queryString>
		<![CDATA[Select a.*,b.* from (
SELECT 1 as pos,'BANK PAYMENT'  ttype,v.Voucher_No,v.Date,cd.Cheque_No,
cd.Cheque_Date,(DrAmount+CrAmount) as amt,v.TransactionWith ,
(Select ch.bookNo from tbChequeBook as ch where ch.folioNo = cd.Cheque_No and ch.ledgerId=$P{bankId}) as bookNo


FROM vwVoucher as v,vwChequeDetails cd
WHERE v.Ledger_Id = $P{bankId} AND v.Voucher_No = cd.Voucher_No AND v.Date
between $P{fromDate} AND $P{toDate} AND v.Voucher_No like 'DR-BK%' AND
 v.companyId = $P!{companyId} AND auditapproveflag = 2 and
 cd.companyId = $P!{companyId}  ) as a

 inner join

 (
	SELECT distinct (Select ll.Ledger_Name from tbLedger as ll where ll.Ledger_Id = vo.Ledger_Id)
 as partyName,vo.Voucher_No
FROM vwVoucher as vo,vwChequeDetails cdo
WHERE vo.Ledger_Id != $P{bankId} AND vo.Voucher_No = cdo.Voucher_No AND vo.Date
between $P{fromDate} AND $P{toDate} AND vo.Voucher_No like 'DR-BK%' AND
 vo.companyId = $P!{companyId} AND auditapproveflag = 2 and
 cdo.companyId = $P!{companyId} ) as b

 on a.Voucher_No = b.Voucher_No

 ORDER BY a.pos,a.Date]]>
	</queryString>
